{
  "name": "c-3po-editor",
  "version": "0.0.1",
  "dependencies": {
    "fuse.js": "^3.2.0",
    "gettext-parser": "^1.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "CI=false react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "pretty": "prettier --write \"./src/**/*.ts\" \"./src/**/*.tsx\" \"./tests/**/*.ts\" \"./tests/**/*.tsx\"",
    "lint": "tslint --fix -c tslint.json src/**/*.ts src/**/*.tsx",
    "tsc": "tsc",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,css,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "homepage": "https://c-3po-org.github.io/c-3po-editor/",
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.2",
    "lint-staged": "^5.0.0",
    "prettier": "1.8.2",
    "react-scripts-ts": "^2.8.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
